<h1>Users</h1>

<table>
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th>Profile</th>
		</tr>
		<% @users.each do |user| %>
		 <tr class="<%= cycle('odd', 'even') %>">
			  <td><%= user.full_name %></td>
			  <td><%= user.email %></td>
			  <td><%= link_to user_path(user), user %></td>
		 </tr>
		<% end %>
</table>
