<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
		<tr>
			<th>Name</th>
			<th>Email</th>
			<th>Profile</th>
		</tr>
		<% @users.each do |user| %>
		<tr class="<%= cycle('odd', 'even') %>">
			<td><%= user.full_name %></td>
			<td><%= user.email %></td>
			<td><%= link_to user_path(user), user %></td>
		</tr>
		</div>
		<% end %>
	</table>
